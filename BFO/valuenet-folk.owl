@prefix : <https://fandaws.com/ontology/bfo/valuenet-folk#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vn-core: <https://fandaws.com/ontology/bfo/valuenet-core.owl#> .
@prefix vn-schwartz: <https://fandaws.com/ontology/bfo/valuenet-schwartz-values.owl#> .
@base <https://fandaws.com/ontology/bfo/valuenet-folk.owl> .

<https://fandaws.com/ontology/bfo/valuenet-folk.owl> rdf:type owl:Ontology ;
                    owl:versionIRI <https://fandaws.com/ontology/bfo/1.0/valuenet-folk.owl> ;
                    owl:imports <https://fandaws.com/ontology/bfo/valuenet-core.owl> ,
                              <https://fandaws.com/ontology/bfo/valuenet-schwartz-values.owl> ;
                    rdfs:comment "An ontology of common-sense or 'folk' human values, aligned with BFO and the ValueNet suite. This module extends the core psychological models with more granular, everyday value concepts." ;
                    rdfs:label "ValueNet Folk Values" ;
                    rdfs:seeAlso <https://w3id.org/valuenet/folk> .

#################################################################
#    Annotation properties
#################################################################

### https://fandaws.com/ontology/bfo/valuenet-folk#definition
skos:definition rdf:type owl:AnnotationProperty ;
            rdfs:subPropertyOf <http://purl.obolibrary.org/obo/IAO_0000115> .

#################################################################
#    Classes
#################################################################

###  https://fandaws.com/ontology/bfo/valuenet-folk#KindnessDisposition
:KindnessDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:BenevolenceDisposition ;
                   skos:definition "A personal value disposition to be friendly, generous, and considerate in action or speech."@en ;
                   rdfs:label "Kindness Disposition"@en ;
                   skos:altLabel "Kindness"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#KindnessDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#LoyaltyDisposition
:LoyaltyDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:BenevolenceDisposition ;
                   skos:definition "A personal value disposition to be faithful and devoted to a person, group, cause, or nation."@en ;
                   rdfs:label "Loyalty Disposition"@en ;
                   skos:altLabel "Loyalty"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#LoyaltyDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#IntegrityDisposition
:IntegrityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:UniversalismDisposition ;
                   skos:definition "A personal value disposition to adhere to moral and ethical principles, demonstrating honesty, consistency, and strong moral character."@en ;
                   rdfs:label "Integrity Disposition"@en ;
                   skos:altLabel "Integrity"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#IntegrityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#CuriosityDisposition
:CuriosityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:StimulationDisposition ;
                   skos:definition "A personal value disposition to explore, investigate, and learn about new things or ideas."@en ;
                   rdfs:label "Curiosity Disposition"@en ;
                   skos:altLabel "Curiosity"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#CuriosityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#ResponsibilityDisposition
:ResponsibilityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:ConformityDisposition ,
                           vn-schwartz:UniversalismDisposition ;
                   skos:definition "A personal value disposition to be reliable, trustworthy, and accountable for one's actions and duties."@en ;
                   rdfs:label "Responsibility Disposition"@en ;
                   skos:altLabel "Responsibility"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#ResponsibilityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#AccountabilityRole
:AccountabilityRole rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:ValueRole ;
                   skos:definition "A value role borne by an agent in a social or professional context, conferring the normative expectation of being answerable for one's actions and decisions."@en ;
                   rdfs:label "Accountability Role"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#AccountabilityRole> .

###  https://fandaws.com/ontology/bfo/valuenet-folk#PunctualityDisposition
:PunctualityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:definition "A personal value disposition to complete a required task or fulfill an obligation before or at a previously designated time."@en ;
                   rdfs:label "Punctuality Disposition"@en ;
                   skos:altLabel "Punctuality"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#PunctualityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#PunctualPersonRole
:PunctualPersonRole rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:ValueRole ;
                   skos:definition "A value role borne by a person in a social context that confers a normative expectation of being on time for appointments or deadlines."@en ;
                   rdfs:label "Punctual Person Role"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#PunctualPersonRole> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#AuthenticityDisposition
:AuthenticityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:SelfDirectionDisposition ;
                   skos:definition "A personal value disposition to represent oneself in a way that is true to one's own personality, spirit, or character, despite external pressures."@en ;
                   rdfs:label "Authenticity Disposition"@en ;
                   skos:altLabel "Authenticity"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#AuthenticityDisposition> .

###  https://fandaws.com/ontology/bfo/valuenet-folk#CreativityDisposition
:CreativityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:StimulationDisposition ;
                   skos:definition "A personal value disposition towards generating or recognizing ideas, alternatives, or possibilities that may be useful in solving problems, communicating with others, and entertaining oneself and others."@en ;
                   rdfs:label "Creativity Disposition"@en ;
                   skos:altLabel "Creativity"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#CreativityDisposition> .


#################################################################
#    Achievement and Growth Cluster
#################################################################

###  https://fandaws.com/ontology/bfo/valuenet-folk#AmbitionDisposition
:AmbitionDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:AchievementDisposition ;
                   skos:definition "A personal value disposition to strive for success, power, or distinction."@en ;
                   rdfs:label "Ambition Disposition"@en ;
                   skos:altLabel "Ambition"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#AmbitionDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#ExcellenceDisposition
:ExcellenceDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:AchievementDisposition ;
                   skos:definition "A personal value disposition to possess qualities to a very high degree and to strive for superiority in performance or outcome."@en ;
                   rdfs:label "Excellence Disposition"@en ;
                   skos:altLabel "Excellence"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#ExcellenceDisposition> .

###  https://fandaws.com/ontology/bfo/valuenet-folk#LearningDisposition
:LearningDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:SelfDirectionDisposition ;
                   skos:definition "A personal value disposition to acquire new knowledge, behaviors, skills, or understanding."@en ;
                   rdfs:label "Learning Disposition"@en ;
                   skos:altLabel "Learning"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#LearningDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#DeterminationDisposition
:DeterminationDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:AchievementDisposition ;
                   skos:definition "A personal value disposition to continue trying to do something, although it is very difficult."@en ;
                   rdfs:label "Determination Disposition"@en ;
                   skos:altLabel "Determination"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#DeterminationDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#GrowthDisposition
:GrowthDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:SelfDirectionDisposition ;
                   skos:definition "A personal value disposition to seek continuous personal development and improvement."@en ;
                   rdfs:label "Growth Disposition"@en ;
                   skos:altLabel "Growth"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#GrowthDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#MasteryDisposition
:MasteryDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:AchievementDisposition ;
                   skos:definition "A personal value disposition to seek comprehensive knowledge or skill in a particular subject or activity."@en ;
                   rdfs:label "Mastery Disposition"@en ;
                   skos:altLabel "Mastery"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#MasteryDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#ResilienceDisposition
:ResilienceDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:definition "A personal value disposition to recover quickly from difficulties or setbacks."@en ;
                   rdfs:label "Resilience Disposition"@en ;
                   skos:altLabel "Resilience"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#ResilienceDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#ResourcefulnessDisposition
:ResourcefulnessDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:StimulationDisposition ;
                   skos:definition "A personal value disposition to find quick and clever ways to overcome difficulties."@en ;
                   rdfs:label "Resourcefulness Disposition"@en ;
                   skos:altLabel "Resourcefulness"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#ResourcefulnessDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#WisdomDisposition
:WisdomDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:UniversalismDisposition ;
                   skos:definition "A personal value disposition to seek and apply experience, knowledge, and good judgment."@en ;
                   rdfs:label "Wisdom Disposition"@en ;
                   skos:altLabel "Wisdom"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#WisdomDisposition> .


#################################################################
#    Community and Social Connection Cluster
#################################################################

###  https://fandaws.com/ontology/bfo/valuenet-folk#AltruismDisposition
:AltruismDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:BenevolenceDisposition ;
                   skos:definition "A personal value disposition to act with selfless concern for the well-being of others."@en ;
                   rdfs:label "Altruism Disposition"@en ;
                   skos:altLabel "Altruism"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#AltruismDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#CompassionDisposition
:CompassionDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:BenevolenceDisposition ;
                   skos:definition "A personal value disposition to recognize the suffering of others and be motivated to alleviate it."@en ;
                   rdfs:label "Compassion Disposition"@en ;
                   skos:altLabel "Compassion"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#CompassionDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#FriendshipDisposition
:FriendshipDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:BenevolenceDisposition ;
                   skos:definition "A personal value disposition to form and maintain bonds of mutual affection with others."@en ;
                   rdfs:label "Friendship Disposition"@en ;
                   skos:altLabel "Friendship"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#FriendshipDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#GenerosityDisposition
:GenerosityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:BenevolenceDisposition ;
                   skos:definition "A personal value disposition to freely give to others, such as time, resources, or kindness."@en ;
                   rdfs:label "Generosity Disposition"@en ;
                   skos:altLabel "Generosity"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#GenerosityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#CooperationDisposition
:CooperationDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:BenevolenceDisposition ,
                          vn-schwartz:UniversalismDisposition ;
                   skos:definition "A personal value disposition to work together with others towards a shared goal."@en ;
                   rdfs:label "Cooperation Disposition"@en ;
                   skos:altLabel "Cooperation"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#CooperationDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#ContributionDisposition
:ContributionDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:UniversalismDisposition ;
                   skos:definition "A personal value disposition to provide service or make a positive impact on a community, cause, or group."@en ;
                   rdfs:label "Contribution Disposition"@en ;
                   skos:altLabel "Contribution"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#ContributionDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#GoodCitizenRole
:GoodCitizenRole rdf:type owl:Class ;
                  rdfs:subClassOf vn-core:ValueRole ;
                  skos:definition "A value role borne by a person within a community or state that confers a normative expectation of contributing to the public good and upholding laws and social norms."@en ;
                  rdfs:label "Good Citizen Role"@en ;
                  rdfs:seeAlso <https://w3id.org/valuenet/folk#GoodCitizenRole> .


#################################################################
#    Integrity and Principle Cluster
#################################################################

###  https://fandaws.com/ontology/bfo/valuenet-folk#HonestyDisposition
:HonestyDisposition rdf:type owl:Class ;
                   rdfs:subClassOf :IntegrityDisposition ;
                   skos:definition "A personal value disposition to be truthful, sincere, and straightforward in communication and action."@en ;
                   rdfs:label "Honesty Disposition"@en ;
                   skos:altLabel "Honesty"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#HonestyDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#DependabilityDisposition
:DependabilityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf :ResponsibilityDisposition ;
                   skos:definition "A personal value disposition to be consistently reliable and trustworthy, such that others can count on one to fulfill commitments."@en ;
                   rdfs:label "Dependability Disposition"@en ;
                   skos:altLabel "Dependability"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#DependabilityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#TrustworthinessDisposition
:TrustworthinessDisposition rdf:type owl:Class ;
                   rdfs:subClassOf :ResponsibilityDisposition ;
                   skos:definition "A personal value disposition to act in a way that makes one worthy of the confidence or trust of others."@en ;
                   rdfs:label "Trustworthiness Disposition"@en ;
                   skos:altLabel "Trustworthiness"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#TrustworthinessDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#FidelityDisposition
:FidelityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf :IntegrityDisposition ;
                   skos:definition "A personal value disposition to be faithful to a person, cause, or belief, demonstrated by continuing loyalty and support."@en ;
                   rdfs:label "Fidelity Disposition"@en ;
                   skos:altLabel "Fidelity"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#FidelityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#SelfRespectDisposition
:SelfRespectDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:SelfDirectionDisposition ;
                   skos:definition "A personal value disposition to hold oneself in esteem and to act in a way that upholds one's own dignity and honor."@en ;
                   rdfs:label "Self-Respect Disposition"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#SelfRespectDisposition> .


#################################################################
#    Security and Stability Cluster
#################################################################

###  https://fandaws.com/ontology/bfo/valuenet-folk#SecurityDisposition
:SecurityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:SecurityDisposition ;
                   skos:definition "A personal value disposition to seek safety, harmony, and stability in society, relationships, and for oneself."@en ;
                   rdfs:label "Security Disposition"@en ;
                   skos:altLabel "Security"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#SecurityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#StabilityDisposition
:StabilityDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:SecurityDisposition ;
                   skos:definition "A personal value disposition to prefer constancy, predictability, and resistance to change."@en ;
                   rdfs:label "Stability Disposition"@en ;
                   skos:altLabel "Stability"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#StabilityDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#OrderDisposition
:OrderDisposition rdf:type owl:Class ;
                  rdfs:subClassOf vn-core:PersonalValueDisposition ;
                  skos:broadMatch vn-schwartz:ConformityDisposition ,
                     vn-schwartz:SecurityDisposition ;
                  skos:definition "A personal value disposition to prefer neatness, organization, and adherence to established structures or arrangements."@en ;
                  rdfs:label "Order Disposition"@en ;
                   skos:altLabel "Order"@en ;
                  rdfs:seeAlso <https://w3id.org/valuenet/folk#OrderDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#ConsistencyDisposition
:ConsistencyDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:ConformityDisposition ;
                   skos:definition "A personal value disposition to act or think in a steady, uniform, and predictable manner over time."@en ;
                   rdfs:label "Consistency Disposition"@en ;
                   skos:altLabel "Consistency"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#ConsistencyDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#TraditionDisposition
:TraditionDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:TraditionDisposition ;
                   skos:definition "A personal value disposition to respect, accept, and commit to the customs or ideas that one's culture or religion provide."@en ;
                   rdfs:label "Tradition Disposition"@en ;
                   skos:altLabel "Tradition"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#TraditionDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#CleanlinessDisposition
:CleanlinessDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:ConformityDisposition ;
                   skos:definition "A personal value disposition to be neat and to keep oneself and one's surroundings clean and free from dirt."@en ;
                   rdfs:label "Cleanliness Disposition"@en ;
                   skos:altLabel "Cleanliness"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#CleanlinessDisposition> .


#################################################################
#    Freedom and Autonomy Cluster
#################################################################

###  https://fandaws.com/ontology/bfo/valuenet-folk#AutonomyDisposition
:AutonomyDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:SelfDirectionDisposition ;
                   skos:definition "A personal value disposition to be self-directing and independent in thought and action."@en ;
                   rdfs:label "Autonomy Disposition"@en ;
                   skos:altLabel "Autonomy"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#AutonomyDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#FreedomDisposition
:FreedomDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-schwartz:SelfDirectionDisposition ;
                   skos:definition "A personal value disposition to value liberty and the absence of necessity, coercion, or constraint in choice or action."@en ;
                   rdfs:label "Freedom Disposition"@en ;
                   skos:altLabel "Freedom"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#FreedomDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#PrivacyDisposition
:PrivacyDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:SelfDirectionDisposition ,
                       vn-schwartz:SecurityDisposition ;
                   skos:definition "A personal value disposition to value being free from public attention, observation, or intrusion."@en ;
                   rdfs:label "Privacy Disposition"@en ;
                   skos:altLabel "Privacy"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#PrivacyDisposition> .


#################################################################
#    Well-being and Harmony Cluster
#################################################################

###  https://fandaws.com/ontology/bfo/valuenet-folk#BalanceDisposition
:BalanceDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:definition "A personal value disposition to seek equilibrium and harmony among different aspects of one's life, such as work, family, and self."@en ;
                   rdfs:label "Balance Disposition"@en ;
                   skos:altLabel "Balance"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#BalanceDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#CalmnessDisposition
:CalmnessDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:SecurityDisposition ;
                   skos:definition "A personal value disposition to remain tranquil, serene, and free from agitation or disturbance."@en ;
                   rdfs:label "Calmness Disposition"@en ;
                   skos:altLabel "Calmness"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#CalmnessDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#HarmonyDisposition
:HarmonyDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:UniversalismDisposition ;
                   skos:definition "A personal value disposition to seek agreement, concord, and a pleasing arrangement of parts, both internally and in social relations."@en ;
                   rdfs:label "Harmony Disposition"@en ;
                   skos:altLabel "Harmony"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#HarmonyDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#JoyDisposition
:JoyDisposition rdf:type owl:Class ;
                rdfs:subClassOf vn-core:PersonalValueDisposition ;
                skos:broadMatch vn-schwartz:HedonismDisposition ;
                skos:definition "A personal value disposition to seek and experience intense happiness, delight, or elation."@en ;
                rdfs:label "Joy Disposition"@en ;
                   skos:altLabel "Joy"@en ;
                rdfs:seeAlso <https://w3id.org/valuenet/folk#JoyDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#OptimismDisposition
:OptimismDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:definition "A personal value disposition to maintain a hopeful and confident attitude about the future or the successful outcome of something."@en ;
                   rdfs:label "Optimism Disposition"@en ;
                   skos:altLabel "Optimism"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#OptimismDisposition> .


###  https://fandaws.com/ontology/bfo/valuenet-folk#PurposeDisposition
:PurposeDisposition rdf:type owl:Class ;
                   rdfs:subClassOf vn-core:PersonalValueDisposition ;
                   skos:broadMatch vn-schwartz:SelfDirectionDisposition ;
                   skos:definition "A personal value disposition to seek a central, self-organizing life aim that stimulates goals and gives life meaning."@en ;
                   rdfs:label "Purpose Disposition"@en ;
                   skos:altLabel "Purpose"@en ;
                   rdfs:seeAlso <https://w3id.org/valuenet/folk#PurposeDisposition> .


